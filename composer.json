{
    "name": "tumedia/aether",
    "description": "Aether - A modular PHP framework",
    "keywords": ["aether"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "filp/whoops": "^2.5",
        "guzzlehttp/psr7": "^1.6",
        "illuminate/config": "^7.30",
        "illuminate/console": "^7.30",
        "illuminate/container": "^7.30",
        "illuminate/contracts": "^7.30",
        "illuminate/database": "^7.30",
        "illuminate/encryption": "^7.30",
        "illuminate/events": "^7.30",
        "illuminate/filesystem": "^7.30",
        "illuminate/support": "^7.30",
        "sentry/sdk": "^3.0",
        "smarty/smarty": "^3.1",
        "symfony/debug": "^4.3",
        "symfony/error-handler": "^5.0@dev",
        "symfony/var-dumper": "^4.3",
        "vlucas/phpdotenv": "^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.4",
        "psy/psysh": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "Aether\\": "./src"
        },
        "files": ["./src/class_aliases.php", "./src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests",
            "Tests\\Fixtures\\": "./tests/Fixtures/src"
        }
    },
    "bin": ["./bin/updateLocales", "./bin/aether"],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
