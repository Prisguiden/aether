{
    "name": "tumedia/aether",
    "description": "Aether - A modular PHP framework",
    "keywords": ["aether"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "filp/whoops": "^2.5",
        "guzzlehttp/psr7": "^1.6",
        "illuminate/config": "^5.8",
        "illuminate/console": "^5.8",
        "illuminate/container": "^5.8",
        "illuminate/contracts": "^5.8",
        "illuminate/database": "^5.8",
        "illuminate/encryption": "^5.8",
        "illuminate/events": "^5.8",
        "illuminate/filesystem": "^5.8",
        "illuminate/support": "^5.8",
        "php-http/curl-client": "^2.0",
        "sentry/sentry": "2.1.*",
        "smarty/smarty": "3.1.34",
        "symfony/debug": "^4.3",
        "symfony/error-handler": "^5.0@dev",
        "symfony/var-dumper": "^4.3",
        "vlucas/phpdotenv": "^3.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.4",
        "psy/psysh": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "Aether\\": "./src"
        },
        "files": ["./src/class_aliases.php", "./src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests",
            "Tests\\Fixtures\\": "./tests/Fixtures/src"
        }
    },
    "bin": ["./bin/updateLocales", "./bin/aether"],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
